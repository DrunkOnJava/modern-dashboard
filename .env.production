# Supabase
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-production-anon-key

# Google Cloud
VITE_GCLOUD_PROJECT_ID=your-production-project-id
VITE_GCLOUD_REGION=us-central1

# Netlify
NETLIFY_SITE_ID=9c42ce6b-49db-4082-a11f-9d20f0670538

# Production Settings
VITE_API_URL=https://api.yourdomain.com
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEBUG_TOOLS=false
VITE_LOG_LEVEL=error

# Feature Flags
VITE_FEATURE_ANALYTICS=true
VITE_FEATURE_REAL_TIME=true
VITE_FEATURE_CLOUD_STORAGE=true

# Performance
VITE_PERFORMANCE_MONITORING=true
VITE_ERROR_REPORTING=true
VITE_CACHE_TTL=7200

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HSTS=true
VITE_RATE_LIMIT=true
VITE_MAX_REQUESTS_PER_MIN=100
